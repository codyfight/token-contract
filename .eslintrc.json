{
  "env": {
    "node": true,
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["unused-imports"],
  "rules": {
    "no-console": "off",
    "unused-imports/no-unused-imports": "error"
  }
}
